{
  "expo": {
    "scheme":"goldrichtop",
    "name": "expo_demo01",
    "slug": "expo_demo01",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon_app.png",
    "splash": {
      "image": "./assets/icon_app.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.goldrichtop.goldrichtop"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_app.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.goldrichtop.goldrichtop"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
